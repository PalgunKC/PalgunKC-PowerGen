
@{
    ViewBag.Title = "SpareQuantityReport";
    Layout = "~/Views/Shared/e_Master.cshtml";
}
@section styles{
    <link href="~/Assets/plugins/select2/css/select2.min.css" rel="stylesheet" />
}
<div ng-app="SpareProductReportApp" ng-controller="SpareProductReportCtrl" ng-cloak>
    <div class="page-header">
        <div class="page-block">
            <div class="row align-items-center">
                <div class="col-md-12">
                    <div class="page-header-title">
                        <h5 class="m-b-10">Daily & MonthWise Report</h5>
                    </div>
                    <ul class="breadcrumb">
                        <li class="breadcrumb-item"><a href="/Dashboard/admin"><i class="feather icon-Dashboard"></i></a></li>
                        <li class="breadcrumb-item"><a href="#!">Report</a></li>
                        <li class="breadcrumb-item"><a href="/Manage_Info/ProductReport">Daily & MonthWise Report</a></li>
                    </ul>
                </div>
            </div>
        </div>
    </div>

    <div class="main-body">
        <div class="page-wrapper">
            <div class="row">
                <div class="col-md-12">
                    <div class="card">

                        <div class="card-body collapse show">
                            <div class="row">
                                <div class="col-sm-4">
                                    <div class="input-group">
                                        <input id="Fromdate" type="text" name="SED" ng-model="obj.FromDate" placeholder="From Date"
                                               class="form-control datepickerFuction" autocomplete="off">
                                        <label class="input-group-addon date-label btn" for="Fromdate">
                                            <span class="input-group-text">
                                                <i class="fas fa-calendar-alt"></i>
                                            </span>
                                        </label>
                                    </div>
                                </div>


                                <div class="col-sm-4">
                                    <div class="input-group">
                                        <input id="Todate" type="text" name="SED" ng-model="obj.Todate" placeholder="To Date" class="form-control datepickerFuction" autocomplete="off">
                                        <label class="input-group-addon date-label btn" for="Todate">
                                            <span class="input-group-text">
                                                <i class="fas fa-calendar-alt"></i>
                                            </span>
                                        </label>
                                    </div>
                                </div>
                                <div class="col-md-4">

                                    <div class="form-group">
                                        @*<label class="form-label">Report Type</label>*@
                                        <select class="form-control selectpicker show-menu-arrow show-tick" id="Return_Status" name="Return_St" ng-model="obj.Report_Type" data-live-search="true" data-actions-box="true" required>
                                            <option value="">--Select Type--</option>
                                            @*<option style="display:none" value="RE">Regrain</option>*@
                                            <option value="MO">Month</option>
                                            <option value="DT">Date</option>
                                        </select>
                                        <p ng-show="ToolReturnForm.Return_St.$invalid && !ToolReturnForm.Return_St.$pristine" class="help-block">This field is required..</p>
                                    </div>
                                </div>
                                <div class="col-md-4">

                                    <div class="form-group">
                                        @*<label class="form-label">Select Type</label>*@
                                        <select class="form-control selectpicker show-menu-arrow show-tick" id="Selected_Type" name="Selected_Type" ng-model="obj.Selected_Type" data-live-search="true" data-actions-box="true" required>
                                            <option value="">--Select Type--</option>
                                            @*<option style="display:none" value="RE">Regrain</option>*@
                                            <option value="Inward">Inward</option>
                                            <option value="Issue">Issue</option>
                                            <option value="Return">Return</option>
                                        </select>
                                        <p ng-show="ToolReturnForm.Selected_Type.$invalid && !ToolReturnForm.Selected_Type.$pristine" class="help-block">This field is required..</p>
                                    </div>
                                </div>

                                <div class="col-md-6">
                                    <div class="form-group">
                                        @*<label class="form-label">Part Description</label>*@
                                        <select class="form-control js-example-basic-multiple col-sm-12 select2-hidden-accessible PartDisc" placeholder="BU" id="PartNumber" data-live-search="true" data-actions-box="true" ng-change="BindPartName(obj,'');ChkReusableQty(obj)" ng-model="obj.PartId" name="PartNumber" ng-options="PartNumber.PartId as (PartNumber.PartName+ '  -  ' +PartNumber.PartNumber+' - '+PartNumber.PartSpecification) for PartNumber in PartMasterList|filter:filterPartMasterList">
                                            @*<option value="" selected="selected">--Choose here--</option>*@
                                        </select>
                                    </div>
                                </div>

                                <div class="col-sm-4">
                                    <div class="input-group-right">
                                        <input type="button" class="btn btn-outline-primary" ng-disabled="!obj.Todate" ng-click="FetchProductReport(obj)" value="View" />
                                        <input type="button" class="btn btn-success" ng-disabled="!obj.Todate" ng-click="GetQuantityReport(obj)" value="Download Excel" />
                                        <input type="button" class="btn btn-outline-danger" ng-click="CancelProductReport()" value="Cancel" />
                                        @*  <a href="#!" class="btn btn-outline-primary" ng-click="GetQuantityReport(obj)"><i class="fas fa-file-excel f-30"></i>&nbsp;Return <span class="ripple ripple-animate" style="height: 60.4875px; width: 60.4875px; animation-duration: 0.7s; animation-timing-function: linear; background: rgb(255, 255, 255); opacity: 0.4; top: -13.6437px; left: -24.3687px;"></span></a>*@
                                    </div>
                                </div>


                            </div>
                            <div class="col-md-12" ng-show="IsListDivVisible">
                                <div class="table-responsive m-t-20">

                                    <div class="dropdown-secondary dropdown d-inline">
                                        <button class="btn btn-primary dropdown-toggle " type="button" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false"><i class="fas fa-bars"></i></button>
                                        <div class="dropdown-menu" data-dropdown-in="fadeIn" data-dropdown-out="fadeOut">
                                            <a class="dropdown-item  " href="#!" ng-click="exportToExcel('#ProductList')"><i class="fas fa-file-excel text-c-blue f-30"></i> Save As Excel</a>
                                            <div class="dropdown-divider"></div>
                                            <a class="dropdown-item  " href="#!" ng-click="exportToPDF('ProductList')"><i class="fas fa-file-pdf text-c-blue f-30"></i> Save As PDF</a>
                                        </div>
                                    </div>

                                    <table class="table table-bordered table-striped mb-0 dataTable no-footer" style="font-weight: bolder;width: 100%" datatable="ng" dt-options="vm.dtOptions" id="ProductList">
                                        <thead>
                                            <tr>
                                                <th>#</th>
                                                @*<th style="display:none">Tool Return id</th>*@
                                                @* <th>S.No</th>*@
                                                <th ng-switch on="ReportType">
                                                    <span ng-switch-when='DT'>  Date</span>
                                                    <span ng-switch-when='MO'>  Month</span>
                                                </th>
                                                @*<th>Sub Inventory</th>*@

                                                <th>Part Number</th>
                                                <th>Part Name</th>
                                                @*<th>Max qty</th>*@
                                                @*<th>Min qty</th>*@
                                                @*<th>InWard</th>*@
                                                @*<th>Cost Per InWard</th>*@
                                                @*<th>Available Qty</th>*@
                                                @*<th>OutWard</th>*@
                                                @*<th>Cost Per OutWard</th>*@
                                                @*<th>Return Qty</th>*@
                                                @*<th>Cost Per Unit</th>*@
                                                @*<th>Stock Value</th>*@
                                                <th>Quantity</th>

                                            </tr>
                                        </thead>

                                        <tbody>
                                            <tr ng-repeat="row in ProductReportList">
                                                <td>{{ $index +   1 }}</td>
                                                @*<td style="display:none">{{ row.ToolReq_id }}</td>*@
                                                @*<td>{{ row.S_No }}</td>*@
                                                <td>{{ row.CreatedAt }}</td>
                                                @*<td>{{ row.SubInv }}</td>*@
                                                <td>{{ row.PartNumber }}</td>
                                                <td>{{ row.PartName }}</td>
                                                @*<td>{{ row.Max_qty }}</td>*@
                                                @*<td>{{ row.Min_qty }}</td>*@
                                                @*<td>{{ row.INWARD }}</td>*@
                                                @*<td>{{ row.Cost_Inward }}</td>*@
                                                @*<td>{{ row.AvailableQty }}</td>*@
                                                @*<td>{{ row.OUTWARD }}</td>*@
                                                @*<td>{{ row.Cost_Outward }}</td>*@
                                                @*<td>{{ row.Return_Qty }}</td>*@
                                                @*<td>{{ row.CostPerUnit }}</td>*@
                                                @*<td>{{ row.Stock_Value}}</td>*@
                                                <td>{{ row.Qty }}</td>

                                            </tr>
                                        </tbody>
                                    </table>

                                </div>

                            </div>
                        </div>
                    </div>
                </div>

                <div class="col-xl-12 col-md-12" ng-show="IsExitDivVisible">
                    <div class="card">
                        <div class="card-header">
                            <h5>InWard&OutWard</h5>
