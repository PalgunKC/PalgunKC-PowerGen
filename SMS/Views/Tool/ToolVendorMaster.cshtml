                                        <thead>
                                            <tr>
                                                <th>#</th>
                                                <th style="display:none">Vendor Id</th>
                                                <th>Vendor Name</th>
                                                <th>Vendor Code</th>
                                                @*<th>Department</th>*@
                                                <th>GST No</th>
                                                <th>Contanct Person</th>
                                                <th>Is Active</th>
                                                <th>Action</th>
                                            </tr>
                                        </thead>

                                        <tbody>

                                            <tr ng-repeat="row in ToolVendorList">
                                                <td>{{row.Row_no}}</td>
                                                <td style="display:none">{{ row.VendorID }}</td>
                                                <td>{{ row.VendorName }}</td>
                                                <td>{{ row.VendorCode }}</td>
                                                <td>{{ row.GSTNo }}</td>
                                                <td>{{ row.Contact_name1 }}</td>
                                                @*<td>{{ row.Department }}</td>*@
